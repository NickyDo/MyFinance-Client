{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/jsx-closing-tag-location": 0,
      "react/prefer-stateless-function": 0,
      "react/no-danger": 0,
      "jsx-a11y/anchor-is-valid": 0,
      "import/prefer-default-export": 0,
      "max-len": 0,
      "semi": 0
    }
  }
  